<%= form_with url:update_order_search_columns_path(format: :json), local: false, class: "update", id: "update_display_search_columns" do %>
  <%= render partial: "index_form" %>
<% end %>
<%= javascript_tag do %>
  $(document).on("succeed.ajaxForm", "#update_display_search_columns", function() {
    window.location.href = '<%= search_columns_path %>'
  });
<% end %>
