<%= form_with model:@classification, html: {class: "col-lg-4"} do |f| %>
  <%= error_notification(@classification.errors) %>

  <div class="form-classification form-classification-sm">
    <%= f.label :parent, class: "small" %>
    <%= f.select :parent_id, Classification.pluck(:full_name,:id), { include_blank: true }, class: "form-control input-sm" %>
  </div>
  <div class="form-classification form-classification-sm">
    <%= f.label :name, class: "small" %>
    <%= f.text_field :name, class: "form-control input-sm" %>
  </div>
  <div class="form-group form-group-sm">
    <%= f.label :sesar_material, class: "small" %>
    <%= f.select :sesar_material, @material.map{|str| [str,str]}, { include_blank: true }, class: "form-control input-sm" %>
  </div>
  <%= render partial: "form_sesar_classification", locals: { collections: @sesar_classification, classification: @classification } %>
  <div class="form-classification form-classification-sm">
    <%= f.label :description, class: "small" %>
    <%= f.text_area :description, class: "form-control input-sm" %>
  </div>

  <%= f.submit "update", class: "btn btn-sm" %>
  <%= link_to "cancel", classifications_path, class: "btn btn-sm" %>
<% end %>

