<%= form_with model:@custom_attribute, html: {class: "col-lg-3"} do |f| %>
  <%= error_notification(@custom_attribute.errors) %>

  <div class="form-group form-group-sm">
    <%= f.label :name, class: "small" %>
    <%= f.text_field :name, class: "form-control input-sm" %>
  </div>
  <div class="form-group form-group-sm">
    <%= f.label :sesar_name, class: "small" %>
    <%= f.text_field :sesar_name, class: "form-control input-sm" %>
  </div>

  <%= f.submit "update", class: "btn btn-sm" %>
  <%= link_to "cancel", custom_attributes_path, class: "btn btn-sm" %>
<% end %>
