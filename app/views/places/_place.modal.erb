<tr>
  <td>
    <%= button_tag class: "btn btn-default btn-xs determine", "data-id" => place.global_id do %>
    <span class="fas fa-link"></span>
    <% end %>
  </td>
  <td><%= link_to place.name, place_path(place, format: :modal), "data-toggle" => "modal", "data-target" => "#show-modal" %></td>
  <td><%= place.latitude_to_text %></td>
  <td><%= place.longitude_to_text %></td>
  <td><%= place.elevation_to_text %></td>
  <td><%= place.specimens_count %></td>
  <td><%= place.group.try!(:name) %></td>
  <td><%= difference_from_now(place.updated_at) %></td>
</tr>

