<%= form_with model:place, local: false, format: :json, html: { id: "new_place", class: "create" } do |f| %>
<tr>
  <th><%= check_box_tag nil, nil, false, class: "select-row-all" %></th>
  <th><%= f.text_field :name,class: "form-control input-sm", class: "form-control input-sm", size: 15 %></th>
  <th><%= f.text_field :latitude,class: "form-control input-sm", class: "form-control input-sm", size: 10 %></th>
  <th><%= f.text_field :longitude,class: "form-control input-sm", class: "form-control input-sm", size: 10 %></th>
  <th><%= f.text_field :elevation,class: "form-control input-sm", class: "form-control input-sm", size: 10 %></th>
  <th></th>
  <%= f.fields_for :record_property_attributes do |r| %>
  <%= th_if_expand @current_user.username %>
  <th>
    <%= r.select :group_id ,Group.pluck(:name, :id), { include_blank: true },class: "form-control input-sm" %>
    <%= r.hidden_field :user_id ,value: @current_user.id %>
  </th>
  <% end %>
  <th>
    <%= f.button class: "btn btn-default" do %>
    <span class="glyphicon glyphicon-save"></span>
    <% end %>
  </th>
  <th></th>
</tr>
<% end %>

